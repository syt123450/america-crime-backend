<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.crime.model.dao.SunDao">

    <select id="getStateData" resultType="com.crime.model.domain.StateTotal">
        SELECT
          state   AS State,
          value   AS total
        FROM sunburst2
        ORDER BY ID
    </select>

    <select id="getMonthData" resultType="Integer">
        SELECT n_involved
        FROM sunburst1
        ORDER BY ID
    </select>

</mapper>